<?xml version="1.0" encoding="utf-8"?>
<com.chen.view.ProgressWheel xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ProgressWheel="http://schemas.android.com/apk/res-auto"
    android:id="@+id/progress_bar"
    android:layout_width="100dp"
    android:layout_height="100dp"
    ProgressWheel:barColor="#339BB9"
    ProgressWheel:barWidth="4dp"
    ProgressWheel:rimColor="#44000000"
    ProgressWheel:rimWidth="4dp"
    ProgressWheel:spinSpeed="3dp"
    ProgressWheel:text="click"
    ProgressWheel:textColor="@color/white"
    ProgressWheel:textSize="14sp"
    android:background="@drawable/bg_upload"
    android:visibility="visible" />
